{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "jasmine": true,
        "node": true,
        "es6": true
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "react/jsx-filename-extension": 0,
        "no-param-reassign": "warn",
        "linebreak-style": "off",
        "no-console": "off",
        "no-underscore-dangle": "off",
    }
}